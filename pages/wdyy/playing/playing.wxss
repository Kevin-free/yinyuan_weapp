/* pages/wdyy/playing/playing.wxss */
.blurbg {
  /* outline: 1px solid pink; */
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  z-index: -1;
  background-size: cover;
  background-position: center;
  filter: blur(20px) brightness(0.6);
  background-repeat: no-repeat;
  transform: scale(1.2);
}

/* 唱针的上一级父容器 */

.con {
  /* outline: 1px solid purple; */
  /* height: 100%; */
  overflow: hidden;
  position: relative;
}

/* 唱针样式 */
.cz {
  /* outline: 1px solid red; */
  width: 222rpx;
  /* height: 366rpx; */
  height: 340rpx;
  position: relative;
  left: 310rpx;
  top: -60rpx;
  z-index: 2;
  transform:rotate(-20deg);
  transition: all 1s ease-in-out;
  animation: cz 1s;
  /* 设置旋转中心（默认以中心）*/
  transform-origin: 52rpx 52rpx;
}

@keyframes cz {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(-20deg);
  }
}

.czplaying{
  /* outline: 1px solid red; */
  width: 222rpx;
  /* height: 366rpx; */
  height: 340rpx;
  position: relative;
  left: 310rpx;
  top: -60rpx;
  z-index: 2;
  transform:rotate(0deg);
  transition: all 1s ease-in-out;
  animation: czplay 1s;
  transform-origin: 52rpx 52rpx;
}

@keyframes czplay {
  from {
    transform: rotate(-20deg);
  }

  to {
    transform: rotate(0deg);
  }
}

/**
 * 黑胶唱片样式效果
 */

#cd-box {
  /* outline: 1px solid red; */
  position: relative;
  top: -200rpx;
  /* top: -20vh; *//* top: -120px; *//* margin-top: 80px; */
}

.level_1 {
  /* outline: 1px solid green; */
  background-color: rgba(255, 255, 255, 0.2);
  margin: 0 auto;
  position: relative;
  /* top: 150rpx; */
  top: 0;
  border-radius: 50%;
  /* 去掉此属性解决轻微直边问题 */
  padding: 1rpx; 
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1;
  width: 520rpx;
  height: 520rpx;
  animation: mymove 25s linear infinite;
  animation-delay: 1s;
  animation-play-state: paused;
}

.playing .level_1 {
  animation-delay: 1s;
  animation-play-state: running !important;
}

@keyframes mymove {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.cd {
  width: 100%;
  height: 100%;
  /* position: fixed; */
  position: relative;
  z-index: 2;
}

.cover {
  width: 65%;
