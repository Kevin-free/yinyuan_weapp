  top: -50rpx;
  display: flex;
  justify-content: center;
}
.download-box{
  /* outline: 1px solid blue; */
  width: 120rpx;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img-download{
  width: 120rpx;
  height: 120rpx;
}

